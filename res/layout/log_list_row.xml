<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:layout_height="wrap_content">
	<LinearLayout android:id="@+id/LinearLayout02"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:padding="5dp" android:layout_gravity="center_vertical">
		<ImageView android:id="@+id/imgSchedule"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:src="@drawable/schedule"></ImageView>
		<ImageView android:id="@+id/imgSpecialEvent"
			android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/gift"></ImageView>
	</LinearLayout>
	<LinearLayout android:id="@+id/LinearLayout03"
		android:layout_height="wrap_content" android:orientation="vertical"
		android:layout_width="fill_parent" android:padding="5dp">
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/txtLogName"
			android:text="Name" android:textColor="#FFFFFF" android:textStyle="bold"
			android:textSize="15sp"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Time" android:textColor="#FFFF00" android:id="@+id/txtLogTime"></TextView>
	<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="desc" android:textColor="#FFFFFF" android:id="@+id/txtLogDesc"></TextView>
</LinearLayout>
</LinearLayout>
